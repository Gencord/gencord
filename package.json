{
  "name": "gencord",
  "version": "0.0.7",
  "description": "A simple, beginner friendly, and easy to use library for interacting with the Discord API, with minimal syntax",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "scripts": {
    "docs": "typedoc --out docs src",
    "test": "tsc && node dist/src/test/test",
    "test:dev": "ts-node-dev --respawn --transpile-only --poll ./src/test/test.ts",
    "test-respawn": "ts-node-dev --respawn --transpile-only src/test/test.ts",
    "lint": "prettier --write .",
    "build": "tsc --diagnostics"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gencord/gencord.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Gencord/gencord/issues"
  },
  "homepage": "https://github.com/Gencord/gencord#readme",
  "devDependencies": {
    "@types/node": "^14.14.20",
    "@types/node-fetch": "^2.5.7",
    "@types/ws": "^7.4.0",
    "glob": "^7.1.6",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "typedoc": "^0.20.13",
    "typedoc-plugin-markdown": "^3.4.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "node-fetch": "^2.6.1",
    "pkg-dir": "^5.0.0",
    "ws": "^7.4.2"
  }
}
